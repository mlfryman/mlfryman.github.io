.port-holder
  .row
    // Project Title
    .project-title
      h1 NashVesTN
        sup
          a#portfolio-close.push-left(href='#', title='Close Project')
            i.fa.fa-times-circle-o
      p description
      section
        .buttons
          a(href='http://nashvestn.herokuapp.com/', title='Go to Website', target='_blank')
            i.fa.fa-2x.fa-external-link-square
          a(href='http://github.com/mlfryman/nashvestn', title='View Source Code', target='_blank')
            i.fa.fa-2x.fa-github-square
        p.tag Website
        p.tag Code
    .row
      div
    .col-md-12
      .portfolio-media
        #project-carousel.owl-carousel.owl-theme
          .item
            img(src='public/assets/img/projects/nashvestn/nashvestn-desk.png', alt='Desktop Admin App')
          .item
            img(src='public/assets/img/projects/nashvestn/nashvestn-mobile.png', alt='Mobile App')


